{
    "name": "Wabbithole",
    "version": "1.0.0",
    "description": "Empowering Curiosity through Wikipedia",
    "manifest_version": 3,
    "author": "Raul Dutta, Nathan Wong, Jack Weitze",
    "action":{
        "default_popup": "index.html",
        "default_title": "Wabbithole"
    },
    "permissions": [
        "tabs",
        "storage",
        "webNavigation"
    ],
    "icons": {
        "16": "wabby16.png",
        "48": "wabby48.png",
        "128": "wabby128.png"
    },
    "background": {
        "service_worker": "background.js",
        "type": "module"
    }
}